{
    "name": "flytachi/extra",
    "description": "Extra Framework",
    "type": "project",
    "license": "MIT",
    "version": "0.1.0",
    "keywords": ["extra", "framework"],
    "scripts": {
        "post-create-project-cmd": [
            "chmod -R 777 storage",
            "@php -r \"file_exists('.env') || copy('extra/Template/Build/env', '.env');\""
        ]
    },
    "autoload": {
        "psr-4": {
            "Flytachi\\Extra\\": "extra/",
            "App\\": "app/"
        },
        "files": [
            "extra/Function/Dependencies.php"
        ]
    },
    "authors": [
        {
            "name": "Flytachi",
            "email": "jasur.rakhmatov03@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "monolog/monolog": "@stable",
        "vlucas/phpdotenv": "@stable",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    }
}
